<?xml version="1.0" encoding="UTF-8"?>
<!--suppress SpringBeanConstructorArgInspection -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">
    <bean id="samlAuthenticationScheme" class="org.gromozeka.teamcity.saml.plugin.SamlAuthenticationScheme"/>
    <bean id="samlLoginExtension" class="org.gromozeka.teamcity.saml.plugin.SamlLoginPageExtension"/>
    <bean id="samlLoginInitiateController" class="org.gromozeka.teamcity.saml.plugin.SamlLoginController"/>
    <bean id="samlLoginCallbackController" class="org.gromozeka.teamcity.saml.plugin.SamlCallbackController"/>
    <bean id="samlPluginAdminPage" class="org.gromozeka.teamcity.saml.plugin.SamlPluginAdminPage"/>
    <bean id="samlPluginSettingsStorage"
          class="org.gromozeka.teamcity.saml.plugin.SamlPluginSettingsStorage"
          init-method="init"/>
    <bean id="samlPluginSettingsController" class="org.gromozeka.teamcity.saml.plugin.SamlPluginAdminPageController"/>
    <bean id="samlAuthenticationSchemeProperties" class="org.gromozeka.teamcity.saml.plugin.SamlAuthenticationSchemeProperties"/>
</beans>